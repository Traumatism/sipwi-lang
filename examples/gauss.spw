brain <- proc [n] do
    n |> gauss_sum |> puts
end

computer <- proc [n] do
    x <- ([1; n] |> sum)
    [1; x] |> range |> sum |> puts
end

main <- proc [] do
    y <- ([1; 200] |> randint)

    "Using next value: " |> puts
    y |> puts
    nl |> puts

    [y] |> brain
    nl |> puts

    [y] |> computer
    nl |> puts
end
